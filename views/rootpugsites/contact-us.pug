    // Contact Form Starts
    section.contact-form.section-padding3
      .container
        .row
          .col-lg-3.mb-5.mb-lg-0
            .d-flex
              .into-icon
                i.fa.fa-home
              .info-text
                h5 California, United States
                p Santa monica bullevard
            .d-flex
              .into-icon
                i.fa.fa-phone
              .info-text
                h5 00 (440) 9865 562
                p Mon to Fri 9am to 6 pm
            .d-flex
              .into-icon
                i.fa.fa-envelope-o
              .info-text
                h5 support@colorlib.com
                p Send us your query anytime!
          .col-lg-9
            form(action='#')
              .left
                input(type='text', placeholder='Enter your name', onfocus="this.placeholder = ''", onblur="this.placeholder = 'Enter your name'", required='')
                input(type='email', placeholder='Enter email address', onfocus="this.placeholder = ''", onblur="this.placeholder = 'Enter email address'", required='')
                input(type='text', placeholder='Enter subject', onfocus="this.placeholder = ''", onblur="this.placeholder = 'Enter subject'", required='')
              .right
                textarea(name='message', cols='20', rows='7', placeholder='Enter Message', onfocus="this.placeholder = ''", onblur="this.placeholder = 'Enter Message'", required='')
              button.template-btn(type='submit') subscribe now
